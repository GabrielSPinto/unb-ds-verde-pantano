container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate"
    leader_only: true
  02_importcsv:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py importcsv ./dados/Leitos_2023.csv DF"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: ebdjango.settings